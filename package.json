{
  "name": "hot-rod",
  "version": "1.0.0",
  "description": "A gui for npm projects",
  "keywords": ["elm", "electron", "gui"],
  "main": "main.js",
  "scripts": {
    "elm": "elm make Main.elm --warn --output elm.js || true",
    "elm:watch": "chokidar '**/*.elm' -c 'npm run elm'",
    "start": "electron main.js",
    "watch": "npm run start & npm run elm:watch & wait"
  },
  "repository": {
    "type": "git",
    "url": "http://www.github.com/ezekeal/hot-rod"
  },
  "author": { "name": "Zach Koch" },
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "electron-prebuilt": "^0.36.7",
    "elm": "^0.16.0"
  }
}
