{
  "name": "hot-rod",
  "version": "1.0.0",
  "description": "A gui for npm projects",
  "keywords": [
    "elm",
    "electron",
    "gui"
  ],
  "main": "main.js",
  "homepage": "http://github.com/ezekeal",
  "scripts": {
    "elm": "elm make Main.elm --warn --output elm.js || true",
    "elm:watch": "chokidar '**/*.elm' -c 'npm run elm'",
    "start": "electron main.js",
    "watch": "npm run start & npm run elm:watch & wait"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/ezekeal/hot-rod"
  },
  "author": {
    "name": "Zach Koch"
  },
  "contributors": [
    {
      "name": "Zach Koch",
      "email": "ezekeal@gmail.com"
    }
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "open": "0.0.5",
    "something": "git://www.something.com",
    "foo" : "1.0.0 - 2.9999.9999"
  , "bar" : ">=1.0.2 <2.1.2"
  , "baz" : ">1.0.2 <=2.3.4"
  , "boo" : "2.0.1"
  , "qux" : "<1.0.0 || >=2.3.1 <2.4.5 || >=2.5.2 <3.0.0"
  , "asd" : "http://asdf.com/asdf.tar.gz"
  , "til" : "~1.2"
  , "elf" : "~1.2.3"
  , "two" : "2.x"
  , "thr" : "3.3.x"
  , "lat" : "latest"
  , "dyl" : "file:../dyl"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "electron-prebuilt": "^1.4.13",
    "elm": "^0.18.0"
  }
}
